@model RentACar.Models.RentalViewModel

@{
    ViewData["Title"] = "Create";
}

<h1>Crear</h1>

<h4>Alquiler</h4>
<hr />
<div class="row">
    <div class="col-md-12">
        <form asp-action="AddRental">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="ReserveId"/>
            <partial name="_Rental" />

            <div class="form-group mt-3">
                <input type="submit" value="Crear" class="btn btn-outline-primary" />
                 <a asp-action="Details" asp-route-id="@Model.ReserveId" class="btn btn-outline-success">Volver</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{
    await Html.RenderPartialAsync("_ValidationScriptsPartial");
}
}
