@model RentACar.Models.RentalTypeViewModel
@{
    ViewData["Title"] = "EditRentalType";
}

<h1>Editar</h1>

<h4>Tipo Alquiler</h4>
<hr />
<div class="row">
    <div class="col-md-12">
        <form asp-action="EditRentalType">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id"/>
            <input type="hidden" asp-for="RentalId"/>
            <partial name="_RentalType" />
            <div class="form-group mt-2">
                <input type="submit" value="Guardar" class="btn btn-outline-primary" />
                <a asp-action="DetailsRental" asp-route-id="@Model.RentalId" class="btn btn-outline-success">Volver</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}


