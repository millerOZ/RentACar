        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DateFinishReserve)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DateFinishReserve)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.PlaceFinishReserve)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PlaceFinishReserve)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.StartReserve)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.StartReserve)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id" class="btn btn-outline-warning">Editar</a>
    <a asp-action="Index" class="btn btn-outline-success">Volver</a>
</div>
